{
    "compilerOptions": {
      "target": "ESNext",
      "moduleResolution": "bundler",
      "module": "esnext",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }
  